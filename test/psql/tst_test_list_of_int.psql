/**
 * @param p_ids   The id's in csv format.
 */
create procedure tst_test_list_of_int( in p_ids varchar(255) )
reads sql data
-- type: rows_with_key tst_id
-- param:p_ids list_of_int , " \
begin
  set @l_sql = CONCAT('select * from TST_LABEL where tst_id in (', p_ids, ');');
  prepare stmt from @l_sql;
  execute stmt
  ;
end
