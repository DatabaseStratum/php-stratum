/** Test for desgination type row1.
    @param p_count The number of rows selected.
                   * 1 For a valid test.
                   * 0 For a invalid test.
                   * 2 For a invalid test.
 */
create procedure tst_test_row1a_with_lob( p_count int,
                                          p_blob  blob )
reads sql data
-- type: row1
begin
  select *
  from   TST_FOO2
  where  tst_c00 <= p_count
  ;
end
