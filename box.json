{
  "chmod": "0755",
  "directories": [
    "lib"
  ],
  "files": [
    "vendor/autoload.php"
  ],
  "finder": [
    {
      "name": "*.php",
      "in": "lib"
    },
    {
      "name": "*.php",
      "in": "vendor/composer"
    },
    {
      "name": "*.php",
      "in": "vendor/phpseclib"
    },
    {
      "name": "*.php",
      "in": "vendor/herrera-io"
    }
  ],
  "compactors": [
    "Herrera\\Box\\Compactor\\Json",
    "Herrera\\Box\\Compactor\\Php"
  ],
  "git-version": "package_version",
  "main": "bin/dlgen",
  "output": "bin/dlgen.phar",
  "stub": true
}
