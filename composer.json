{
  "name": "setbased/php-stratum",
  "description": "A stored procedure and function loader, wrapper generator and more for MySQL in PHP.",
  "keywords": ["MySQL","Stored Routines","Stored Procedures","Stored Functions","Wrapper","Loader","PHP"],
  "type": "tool",
  "license": "MIT",
  "require": {
    "php": ">=5.3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "3.*",
    "setbased/phing-extensions": "1.*"
  },
  "require": {
    "phpdocumentor/reflection-docblock": "2.*",
    "setbased/php-affirm": "*"
  },
  "bin": [
    "bin/stratum"
  ],
  "autoload": {
    "psr-4": {
      "SetBased\\PhpStratum\\": "PhpStratum/"
    }
  },
  "config": {
    "bin-dir": "bin"
  }
}